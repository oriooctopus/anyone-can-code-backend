    "HintOld": {
      "type": "component",
      "repeatable": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "code-challenge.hint",
      "required": false
    },
    "internalNotes": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "text",
      "private": true
    },
    "hints": {
      "type": "component",
      "repeatable": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "challenge.hint",
      "required": false
    },
    "getStartingCodeFromPreviousChallenge": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "boolean",
      "default": false
    },
    "sharedEvaluationCode": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "richtext"
    }
  }
