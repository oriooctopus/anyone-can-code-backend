{
  "kind": "collectionType",
  "collectionName": "sublessons",
  "info": {
    "name": "Sublesson",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "slug": {
      "type": "string"
    },
    "lesson": {
      "model": "lesson"
    },
    "name": {
      "type": "string",
      "required": true,
      "unique": true,
      "default": "A variable is like a label"
    },
    "challenges": {
      "type": "component",
      "repeatable": true,
      "component": "sublessonchallenge.challenge"
    },
    "descriptions": {
      "type": "component",
      "repeatable": false,
      "component": "sublesson.sublesson-descriptions",
      "required": true
    },
    "sublesson_challenges": {
      "collection": "sublesson-challenge"
    },
    "sublesson_challenges_test": {
      "via": "sublesson",
      "collection": "sublesson-challenge"
    }
  }
}
