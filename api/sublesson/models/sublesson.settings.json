{
  "kind": "collectionType",
  "collectionName": "sublessons",
  "info": {
    "name": "Sublesson",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "challenges": {
      "type": "component",
      "repeatable": true,
      "component": "sublessonchallenge.challenge"
    },
    "descriptions": {
      "type": "component",
      "repeatable": false,
      "component": "sublesson.sublesson-descriptions",
      "required": true
    },
    "lesson": {
      "via": "sublessons",
      "model": "lesson"
    },
    "InternalMaintenanceNotes": {
      "type": "text",
      "private": true
    },
    "persistCodeBetweenChallenges": {
      "type": "boolean",
      "default": false
    },
    "minimizeEditor": {
      "type": "boolean",
      "default": false
    },
    "syntaxEntry": {
      "model": "syntax-entry",
      "via": "sublesson"
    }
  }
}
