{
  "kind": "collectionType",
  "collectionName": "syntax_entries",
  "info": {
    "name": "SyntaxEntry",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "content": {
      "type": "richtext",
      "required": true
    },
    "name": {
      "type": "string",
      "private": false,
      "required": true
    },
    "sublesson": {
      "via": "syntaxEntry",
      "model": "sublesson"
    },
    "InternalNotes": {
      "type": "text",
      "private": true
    },
    "lesson": {
      "model": "lesson",
      "via": "syntaxEntry"
    },
    "maxWidth": {
      "type": "integer",
      "default": 0,
      "required": false,
      "unique": false
    }
  }
}
